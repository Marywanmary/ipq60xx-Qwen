## 通用配置文件

CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y
# CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=""
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="luci-app-athena-led luci-i18n-athena-led-zh-cn"

CONFIG_USE_APK=n							# 禁用Android APK包管理器
CONFIG_DEVEL=y								# 启用开发者模式
CONFIG_TARGET_OPTIONS=y						# 启用目标平台特定选项
CONFIG_TARGET_OPTIMIZATION="-O3 -pipe -march=armv8-a+crypto+crc -mcpu=cortex-a53+crypto+crc -mtune=cortex-a53"

# CONFIG_ATH11K_MEM_PROFILE_512M=y		# 内核配置选项，用于控制驱动内存分配策略。它应直接在内核配置中设置，而非作为设备软件包的值传递，在多设备配置中不建议使用。
# CONFIG_ATH11K_MEM_PROFILE_1G=y		# 若需为不同设备使用不同内存配置，需修改设备定义的Makefile（如target/linux/qualcommax/ipq60xx/image/Makefile
CONFIG_NSS_FIRMWARE_VERSION_12_5=y

CONFIG_CCACHE=y
CONFIG_PACKAGE_default-settings=y
CONFIG_PACKAGE_default-settings-chn=y
CONFIG_TARGET_PER_DEVICE_ROOTFS=y
# CONFIG_LUCI_LANG_zh_Hant=y

# CONFIG_FEED_nss_packages is not set
# CONFIG_FEED_sqm_scripts_nss is not set
# CONFIG_TARGET_ROOTFS_INITRAMFS is not set

CONFIG_PACKAGE_kmod-usb2=y					# 启用USB 2.0支持
CONFIG_PACKAGE_kmod-usb3=y					# 启用USB 3.0支持
CONFIG_PACKAGE_kmod-usb-ohci=y				# 启用USB OHCI控制器
CONFIG_PACKAGE_kmod-usb-uhci=y				# 启用USB UHCI控制器
CONFIG_PACKAGE_kmod-usb-ehci=y				# 启用USB EHCI控制器
CONFIG_PACKAGE_kmod-usb-xhci=y				# 启用USB xHCI控制器

CONFIG_PACKAGE_shadow-useradd=y			#基本的用户创建，不会自动创建用户主目录、密码、添加到默认组
# 示例：useradd -m -d /home/testuser -s /bin/sh testuser  #passwd newuser
CONFIG_PACKAGE_adduser=y				#提供交互式用户创建，自动创建用户主目录、密码、添加到默认组，友好的用户界面

CONFIG_PACKAGE_tmux=y						# 终端复用工具
CONFIG_PACKAGE_dmesg=y						# 内核日志查看器
CONFIG_PACKAGE_htop=y						# 进程监控工具(文本)
CONFIG_PACKAGE_btop=y						# 进程监控工具(图形)
CONFIG_PACKAGE_fdisk=y						# 磁盘分区工具(MBR)
CONFIG_PACKAGE_cfdisk=y						# 交互式磁盘分区工具
CONFIG_PACKAGE_sgdisk=y						# GPT分区管理工具
CONFIG_PACKAGE_tune2fs=y					# ext文件系统调整工具
CONFIG_PACKAGE_ariang=y						# Aria2 Web界面
CONFIG_PACKAGE_fstrim=y						# SSD优化工具(TRIM)
CONFIG_PACKAGE_nano-full=y					# 增强版nano编辑器
CONFIG_PACKAGE_autocore=y					# CPU核心管理工具
CONFIG_PACKAGE_coremark=y					# CPU性能测试工具
CONFIG_PACKAGE_zram-swap=y					# 内存压缩交换工具
CONFIG_PACKAGE_kmod-lib-lz4=y				# LZ4压缩内核模块
CONFIG_PACKAGE_hdparm=y						# 硬盘参数测试工具
CONFIG_PACKAGE_zoneinfo-asia=y				# 亚洲时区数据
CONFIG_PACKAGE_openssh-sftp-server=y		# SSH文件传输服务器
# CONFIG_PACKAGE_acme-acmesh-dnsapi=y		# 禁用ACME证书DNS插件
# CONFIG_PACKAGE_ddns-scripts-cloudflare=y	# 禁用Cloudflare DDNS脚本
# CONFIG_PACKAGE_smartmontools-drivedb=y	# 禁用硬盘监控数据库
# CONFIG_PACKAGE_sqm-scripts-nss=y			# 禁用NSS加速流量整形

# === 基础Luci包 ===
CONFIG_PACKAGE_luci-app-netspeedtest=y		# 网络速度测试工具
CONFIG_PACKAGE_luci-app-openclash=y			# OpenClash代理客户端
# CONFIG_PACKAGE_luci-app-partexp=y		# 禁用磁盘分区扩展工具
CONFIG_PACKAGE_luci-app-samba4=y			# Samba文件共享服务
CONFIG_PACKAGE_tailscale=y					# Tailscale VPN客户端
CONFIG_PACKAGE_luci-app-tailscale=y			# Tailscale管理界面
# CONFIG_PACKAGE_luci-i18n-tailscale-zh_cn=y	# 禁用Tailscale中文包
CONFIG_PACKAGE_luci-app-taskplan=y			# 任务计划管理工具
CONFIG_PACKAGE_luci-app-ttyd=y				# Web终端工具
CONFIG_PACKAGE_luci-app-upnp=y				# UPnP端口转发
CONFIG_PACKAGE_luci-app-wolplus=y			# 高级网络唤醒工具
